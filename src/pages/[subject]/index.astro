---
import NewPage from "../../components/NewPage.astro";
import { subjectIndexPaths } from "../../paths";

export async function getStaticPaths() {
  const excludedSubjects = ["fizica","admitere"];
  return subjectIndexPaths(excludedSubjects);
}

const { subject } = Astro.params as { subject: string };
const { firstPage } = Astro.props as { firstPage: string };
---

<NewPage subject={subject} page={firstPage} />
