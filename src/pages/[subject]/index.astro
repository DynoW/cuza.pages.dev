---
import Page from "../../components/Page.astro";
import { getFirstPage, generateSubjectIndexPaths } from "../../utils/paths";

export function getStaticPaths() {
  const excludedSubjects = ["fizica","admitere"];
  return generateSubjectIndexPaths(excludedSubjects);
}

// Get the dynamic route parameter
const { subject } = Astro.params;
const firstPage = getFirstPage(subject!);
---

<Page subject={subject} page={firstPage} />
