---
import NewPage from "../../components/NewPage.astro";
import { subjectPagePaths } from "../../paths";

export const getStaticPaths = subjectPagePaths;

const subject = Astro.params?.subject;
const page = Astro.params?.page;

if (!subject || !page) {
  throw new Error(`Subject and page parameters are required. Received params: ${JSON.stringify(Astro.params)}`);
}
---

<NewPage subject={subject} page={page} />
